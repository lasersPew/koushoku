<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {{- range .archives }}
    <url>
      <loc>{{ baseURL }}/archive/{{ .ID }}/{{ .Slug }}</loc>
      <lastmod>{{ formatUnix .UpdatedAt "2006-01-02" }}</lastmod>
    </url>
  {{- end }}
</urlset>